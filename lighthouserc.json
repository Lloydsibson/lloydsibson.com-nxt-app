{
  "ci": {
    "collect": {
      "url": ["http://localhost:3000"],
      "startServerCommand": "serve:lhci",
      "startServerReadyPattern": "Server is listening",
      "settings": {
        "onlyCategories": ["performance"]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
